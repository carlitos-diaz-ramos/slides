{
  "compilerOptions": {
    "strict": true,
    "lib": ["es2015", "dom", "dom.iterable"],
    "target": "es2015",
    "module": "NodeNext",
    "rewriteRelativeImportExtensions": true,
    "outDir": "modules",
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "noImplicitThis": true, 
    "strictNullChecks": true,
    "moduleResolution": "nodenext",
  },
  "include": ["ts"],
}
